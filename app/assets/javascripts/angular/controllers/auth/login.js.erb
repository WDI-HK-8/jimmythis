app.controller('LogInCtrl', ['$scope','$auth', function($scope, $auth){
  $scope.message = "Login";

  $scope.login = function() {
    $auth.submitLogin($scope.loginForm).then(function(response) {
      console.log(response);
      $location.path('/');
    }).catch(function(response) {
      console.log(response);
    });
  }
}]);